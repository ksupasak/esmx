<h1>Edit Project</h1>

<%= form_for(:project,:remote=>true) do |f| %>
<%= hidden_field_tag :id, @project.id %>
<%= render :partial=> 'project', :locals=>{:f=>f} %>
<br/>
<%= f.submit 'Update' , :class=>bt_class%> | <%= link_to 'Cancel', :action=>'show',:id=>@project%> | <%= link_to 'Content' , "/esm_content/#{@project.id}",:remote=>true ,:disable_with=>'loading..'%>
<% end %>